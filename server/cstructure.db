BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS "Users" (
	"user_id"	INTEGER,
	"username"	TEXT NOT NULL UNIQUE,
	"hash"	TEXT NOT NULL,
	"salt"	TEXT NOT NULL,
	"is_loyal_customer"	BOOLEAN DEFAULT 0,
	PRIMARY KEY("user_id" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "Theaters" (
	"theater_id"	INTEGER,
	"name"	TEXT NOT NULL,
	"rows"	INTEGER NOT NULL,
	"seats_per_row"	INTEGER NOT NULL,
	PRIMARY KEY("theater_id" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "Concerts" (
	"concert_id"	INTEGER,
	"name"	TEXT NOT NULL,
	"theater_id"	INTEGER NOT NULL,
	PRIMARY KEY("concert_id" AUTOINCREMENT),
	FOREIGN KEY("theater_id") REFERENCES "Theaters"("theater_id")
);
CREATE TABLE IF NOT EXISTS "Seats" (
	"seat_id"	INTEGER,
	"theater_id"	INTEGER NOT NULL,
	"row_number"	INTEGER NOT NULL,
	"seat_position"	TEXT NOT NULL,
	UNIQUE("theater_id","row_number","seat_position"),
	PRIMARY KEY("seat_id" AUTOINCREMENT),
	FOREIGN KEY("theater_id") REFERENCES "Theaters"("theater_id")
);
CREATE TABLE IF NOT EXISTS "ConcertSeats" (
	"concert_seat_id"	INTEGER,
	"concert_id"	INTEGER NOT NULL,
	"seat_id"	INTEGER NOT NULL,
	"status"	TEXT DEFAULT 'available',
	UNIQUE("concert_id","seat_id"),
	PRIMARY KEY("concert_seat_id" AUTOINCREMENT),
	FOREIGN KEY("concert_id") REFERENCES "Concerts"("concert_id"),
	FOREIGN KEY("seat_id") REFERENCES "Seats"("seat_id")
);
CREATE TABLE IF NOT EXISTS "Reservations" (
	"reservation_id"	INTEGER,
	"user_id"	INTEGER NOT NULL,
	"concert_id"	INTEGER NOT NULL,
	"reserved_at"	DATETIME DEFAULT CURRENT_TIMESTAMP,
	UNIQUE("user_id","concert_id"),
	PRIMARY KEY("reservation_id" AUTOINCREMENT),
	FOREIGN KEY("concert_id") REFERENCES "Concerts"("concert_id"),
	FOREIGN KEY("user_id") REFERENCES "Users"("user_id")
);
CREATE TABLE IF NOT EXISTS "ReservationSeat" (
	"reservation_id"	INTEGER NOT NULL,
	"concert_seat_id"	INTEGER NOT NULL,
	PRIMARY KEY("reservation_id","concert_seat_id"),
	FOREIGN KEY("concert_seat_id") REFERENCES "ConcertSeats"("concert_seat_id"),
	FOREIGN KEY("reservation_id") REFERENCES "Reservations"("reservation_id")
);
